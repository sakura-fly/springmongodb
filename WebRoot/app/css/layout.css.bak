			/*
	  * 外面盒子样式---自己定义
	  */
	
	.page_div {
		margin-top: 20px;
		margin-bottom: 20px;
		font-size: 15px;
		font-family: "microsoft yahei";
		color: #666666;
		margin-right: 10px;
		padding-left: 20px;
		box-sizing: border-box;
	}
	/*
	 * 页数按钮样式
	 */
	.page_div a {
		min-width: 30px;
		height: 28px;
		border: 1px solid #dce0e0!important;
		text-align: center;
		margin: 0 4px;
		cursor: pointer;
		line-height: 28px;
		color: #666666;
		font-size: 13px;
		display: inline-block;
	}
	#firstPage,
	#lastPage {
		width: 50px;
		color: #00a2ca;
		border: 1px solid #00a2ca!important;
	}
	#prePage,
	#nextPage {
		width: 70px;
		color: #00a2ca;
		border: 1px solid #00a2ca!important;
	}
	.page_div .current {
		background-color: #00a2ca;
		border-color: #00a2ca;
		color: #FFFFFF;
	}
	.totalPages {
		margin: 0 10px;
	}
	.totalPages span,
	.totalSize span {
		color: #00a2ca;
		margin: 0 5px;
	}

.layout-pager{position:relative;top:5px;bottom:10px;}
.layout-pager .pager-total {	float:left;left:10px;position:absolute; font-size:13px;}
.layout-pager .pager-inner {	float:left;	font-size:13px;margin-bottom:12px;}
.layout-pager a,.layout-pager span {	float:left;	width:27px;	height:27px;margin-right:5px;	line-height:27px;	overflow:hidden;	border:0px solid #e7ecf0;	background:#fff;text-decoration:none;text-align:center;background:#f6f6f6;}
.layout-pager a:hover{background:#e9f1f7;text-decoration:none;color:#0000cd}
.layout-pager .current{width:27px;border:none;background:0 0;font-weight:700;color:#000}
.layout-pager span{background:#f6f6f6; display:block; width:27px;height:27px;line-height:27px;border:none;}
.layout-pager .first, .layout-pager .last{width:41px;*line-height:24px;line-height:24px\9\0}
.layout-pager .prev,  .layout-pager .next{width:60px;line-height:23px;_line-height:24px;line-height:24px\9\0;}
.layout-pager .nums{float:right;margin-left:65px;font-size:12px;color:#999}
.pager-center .pager{float:left;position:relative;left:50%}
.pager-center .pager-inner{position:relative;right:50%}
.layout-pager a{width:27px;height:27px;line-height:27px}
.layout-pager .next,    .layout-pager .prev,    .layout-pager .first,    .layout-pager .last{line-height:27px;width:60px;}
.layout-pager .current{height:27px;line-height:27px;color:#fff;font-weight:400;background:#28b385}
.layout-pager a{color:#fff;background:#1d66c4;}
.layout-pager a:hover{color:#fff;background:#ff0000}


